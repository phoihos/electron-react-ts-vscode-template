{
  "name": "electron-react-ts-vscode-template",
  "displayName": "electron-react-ts-vscode-template",
  "description": "A React-based Electron template project written with Typescript",
  "version": "0.0.1",
  "license": "CC0-1.0",
  "repository": "https://github.com/phoihos/electron-react-ts-vscode-template",
  "author": {
    "name": "phoihos"
  },
  "homepage": "./",
  "private": true,
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.example.app",
    "productName": "HelloWorld",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prebuild": "tsc -p tsconfig.prebuild.json",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "wait-electron": "wait-on tcp:9223"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.15",
    "@types/react": "^16.14.3",
    "@types/react-dom": "^16.9.10",
    "electron-is-dev": "^1.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "electron": "^11.2.2",
    "electron-builder": "^22.9.1",
    "wait-on": "^5.2.1"
  }
}
