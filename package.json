{
  "name": "electron-react-ts-vscode-template",
  "displayName": "electron-react-ts-vscode-template",
  "description": "A React-based Electron template project written with Typescript",
  "version": "0.0.1",
  "license": "CC0-1.0",
  "repository": "https://github.com/phoihos/electron-react-ts-vscode-template",
  "author": {
    "name": "phoihos"
  },
  "homepage": "./",
  "private": true,
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.example.app",
    "productName": "HelloWorld",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prebuild": "tsc -p tsconfig.prebuild.json",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "wait-electron": "wait-on tcp:9223"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "electron-is-dev": "^1.1.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "electron": "^8.1.1",
    "electron-builder": "^22.4.0",
    "wait-on": "^4.0.1"
  }
}
