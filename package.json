{
  "name": "electron-react-ts-vscode-template",
  "displayName": "electron-react-ts-vscode-template",
  "description": "A React-based Electron template project written with Typescript",
  "version": "0.0.1",
  "license": "CC0-1.0",
  "repository": "https://github.com/phoihos/electron-react-ts-vscode-template",
  "author": {
    "name": "phoihos"
  },
  "homepage": "./",
  "private": true,
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.example.app",
    "productName": "HelloWorld",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prebuild": "tsc -p tsconfig.prebuild.json",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "wait-electron": "wait-on tcp:9223"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^14.18.21",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "electron-is-dev": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.1.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "electron": "^17.4.8",
    "electron-builder": "^23.1.0",
    "wait-on": "^6.0.1"
  }
}
