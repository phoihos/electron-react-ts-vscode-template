{
  "name": "electron-react-ts-vscode-template",
  "displayName": "electron-react-ts-vscode-template",
  "description": "A React-based Electron template project written with Typescript",
  "version": "0.0.1",
  "license": "CC0-1.0",
  "repository": "https://github.com/phoihos/electron-react-ts-vscode-template",
  "author": {
    "name": "phoihos"
  },
  "homepage": "./",
  "private": true,
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.example.app",
    "productName": "HelloWorld",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prebuild": "tsc -p tsconfig.prebuild.json",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "wait-electron": "wait-on tcp:9223"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.1",
    "@types/node": "^16.11.62",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "electron-is-dev": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.8.2",
    "web-vitals": "^3.0.2"
  },
  "devDependencies": {
    "electron": "^19.1.1",
    "electron-builder": "^23.3.3",
    "wait-on": "^6.0.1"
  }
}
